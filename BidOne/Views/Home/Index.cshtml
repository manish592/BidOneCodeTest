@model BidOne.ViewModels.HomeViewModel
@{
    ViewData["Title"] = "Home Page";
}
<link type="text/css" rel="stylesheet" href="@Url.Content(string.Format("~/lib/fontawesome-free/css/all.min.css?v={0}",DateTime.Now.Ticks))">
<link type="text/css" rel="stylesheet" href="@Url.Content(string.Format("~/lib/ionicons/css/ionicons.min.css?v={0}",DateTime.Now.Ticks))">
<link type="text/css" rel="stylesheet" href="@Url.Content(string.Format("~/lib/typicons.font/typicons.css?v={0}",DateTime.Now.Ticks))">
<link type="text/css" rel="stylesheet" href="@Url.Content(string.Format("~/css/azia.css?v={0}",DateTime.Now.Ticks))">
<body>
    <div class="text-center">
        <h1 class="display-4">Welcome</h1>
        <form asp-action="Signup" asp-antiforgery="true" autocomplete="off" method="post">
            <div class="form-group">
                <input asp-for="RegistertViewModel.Firstname" class="form-control" placeholder="Enter your firstname" />
                <span asp-validation-for="RegistertViewModel.Firstname" class="text-danger"></span>
            </div><!-- form-group -->
            <div class="form-group">
                <input asp-for="RegistertViewModel.Lastname" class="form-control" placeholder="Enter your lastname" />
                <span asp-validation-for="RegistertViewModel.Lastname" class="text-danger"></span>
            </div>
            <button type="submit" name="actionButton" id="Register" value="Submit" class="btn btn-az-primary btn-block">Submit</button>
        </form>
    </div><!-- form-group -->
    <script src="@Url.Content("~/lib/jquery/jquery.min.js")"></script>
    <script src="@Url.Content("~/lib/bootstrap/js/bootstrap.bundle.min.js")"></script>
    <script src="@Url.Content("~/lib/ionicons/ionicons.js")"></script>
    <script src="@Url.Content("~/js/jquery.cookie.js")"></script>
    <script src="@Url.Content("~/js/jquery.cookie.js")"></script>
    <script src="@Url.Content("~/js/azia.js")"></script>
    <script>
        $(function () {
            'use strict'

        });</script>
</body>

@{
    await Html.RenderPartialAsync("_ValidationScriptsPartial");
}

